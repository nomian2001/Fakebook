<section class="container-fluid">
  <div class="row"> 
    <div class="col">
      <%= render "shared/profile",  user: @user %>
      <%= render "friendships/listfriends", user: @user%>
    </div>
    <div class="col-5">
      <%= render "shared/feed"  %>
    </div>
    <div class="col">
      <% if @user.id == current_user.id%>
        <%= render "friendships/strangers", strangers: @strangers %>
      <%end%>
    </div>
  </div>
</section>